set srcpath src/
set sqlpath sql/

set usingforeignkeys true
set dbtype mysql
set dbuser root
-set dbpassword 
set dbserver localhost
set dbport 3306
set dbname foo

set modelpackage foo.model
set usingannotations true

set controllerpackage foo.controller
set controllerclass BasicController
set controllerformat %sController

set parserpackage foo.parser
set parserformat %sParser

set entitymodelpackage foo.entitymodels
set entitymodelformat %sEntityModel
set entitymodelfactoryclass EntityModelFactory

begin entity FooProp
	prop1 varchar(40) "Prop1"
	prop2 varchar(40) "Prop2"
	prop3 varchar(40) "Prop3"
end entity

begin entity Foo
	fooVarchar varchar(50) ""
	fooDate date 
	fooNumeric numeric(9,2) 1234567.89
	fooChar char(30) "foo"
	onetoone FooProp
end entity

begin entity Bar
	barVarchar varchar(20) "bar"
	barInt int 45
	manytoone Foo
end entity

begin entity FooBar
	foobarstring varchar(30) "foobar"
	manytoone Foo
	manytoone Bar myBar
end entity

create sqlcreate
create hibernate
create javamodels
create javaparser
create javacontroller
create javaentitymodels

