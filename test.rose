set annotations true
set modelpackage foo
set srcpath src/
set sqlpath sql/
set foreignkeys true

begin entity FOO
	FOO_VARCHAR varchar(50) 
	FOO_DATE date getDate()
	FOO_NUMERIC numeric(9,2) 1234567.89
	FOO_CHAR char(5) 'def'
end entity

begin entity BAR
	BAR_VARCHAR varchar 20
	BAR_INT int
	FOO
end entity

begin entity FOOBAR
	FOO
	MYBAR BAR
end entity

create sqlcreate
create sqldelete
create javamodels
