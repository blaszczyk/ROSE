set srcpath src/
set sqlpath sql/

set usingforeignkeys true
set database foo

set modelpackage foo
set usingannotations true

set panelpackage foo.panels
set fullpanelformat %sFullPanel
set simplepanelformat %sSimplePanel

set labelheigth 30
set labelspacing 10
set labelwidth 150

begin entity Foo
	fooVarchar varchar(50) null
	fooDate date 
	fooNumeric numeric(9,2) 1234567.89
	fooChar char(5) "foo"
end entity

begin entity Bar
	barVarchar varchar(20) "bar"
	barInt int 45
	Foo
end entity

begin entity FooBar
	Foo
	myBar Bar
end entity

create sqlcreate
create javamodels
create javasimplepanels
create javafullpanels