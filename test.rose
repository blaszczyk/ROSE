set annotations true
set modelpackage foo
set srcpath src/
set sqlpath sql/
set foreignkeys true
set database foo

begin entity Foo
	fooVarchar varchar(50) null
	fooDate date 
	fooNumeric numeric(9,2) 1234567.89
	fooChar char(5) "foo"
end entity

begin entity Bar
	barVarchar varchar(20) "bar"
	barInt int 45
	Foo
end entity

begin entity FooBar
	Foo
	myBar Bar
end entity

create sqlcreate
create javamodels
